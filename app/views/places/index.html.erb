<%= javascript_include_tag "https://maps.googleapis.com/maps/api/js?libraries=places&key=#{ENV['GMAPS_API_BROWSER_KEY']}" %>
<div class="nav-submit-wrapper">
  <%= react_component("Navbar", {}) %>
  <div class="submit-location">
    <%= link_to new_place_path do %>
      <%= react_component("Button", {}) %>
    <% end %>
  </div>
</div>
<%= react_component("Map", {
  center: @map_center,
  zoom: @map_zoom,
  markers: [@markers]
}) %>
<%= react_component("Tabs", {places: @places}) %>
<%= react_component("Footer", {}) %>
